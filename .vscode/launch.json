    "version": "0.2.0",
    "configurations": [
        {
            "name": "go-assistant by command",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "-cmd=operate",
                "-source=redis",
                "-url=cluster://:@192.168.2.158:7001",
                "-opt=keys",
                "-regexp=liyunpeng_dev_local_Data_*",
            ]
        },
        {
            "name": "go-assistant by tview",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "-dev=true",
            ]
        }
    ]
}